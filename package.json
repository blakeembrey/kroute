    "email": "hello@blakeembrey.com",
    "url": "http://blakeembrey.me"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blakeembrey/kroute/issues"
  },
  "homepage": "https://github.com/blakeembrey/kroute",
  "devDependencies": {
    "co-mocha": "^1.1.0",
    "istanbul": "^0.4.5",
    "koa": "^2.2.0",
    "mocha": "^3.3.0",
    "popsicle": "^9.1.0",
    "popsicle-server": "^2.0.0",
    "standard": "^10.0.2"
  },
  "dependencies": {
    "array-flatten": "^2.1.1",
